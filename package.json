{
	"name": "code-annotator",
	"displayName": "code-annotator",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-annotator.clear",
				"title": "Clear"
			},
			{
				"command": "code-annotator.done",
				"title": "Done"
			},
			{
				"command": "code-annotator.na",
				"title": "Not applicable"
			},
			{
				"command": "code-annotator.obsolete",
				"title": "Obsolete"
			}
		],
		"keybindings": [
			{
				"command": "code-annotator.clear",
				"key": "ctrl+numpad0",
				"mac": "cmd+numpad0",
				"when": "editorTextFocus"
			},
			{
				"command": "code-annotator.done",
				"key": "ctrl+numpad1",
				"mac": "cmd+numpad1",
				"when": "editorTextFocus"
			},
			{
				"command": "code-annotator.na",
				"key": "ctrl+numpad2",
				"mac": "cmd+numpad2",
				"when": "editorTextFocus"
			},
			{
				"command": "code-annotator.obsolete",
				"key": "ctrl+numpad3",
				"mac": "cmd+numpad3",
				"when": "editorTextFocus"
			},
			{
				"command": "code-annotator.stats",
				"key": "ctrl+numpad4",
				"mac": "cmd+numpad4",
				"when": "editorTextFocus"
			}
		]
	},
	"extensionDependencies": [
		"vscode.git"
	],
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.16.0",
		"glob": "8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {}
}
